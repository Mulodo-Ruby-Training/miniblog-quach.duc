<!-- View to show all users in database -->
<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <h1 class="page-header">List All User</h1>
      <ol class="breadcrumb">
        <li><a href="/">Home</a>
        </li>
        <li class="active" href="/list_users">List All User</li>
      </ol>
    </div>
  </div>
  <div class="row">
    <div class="table-responsive">
      <table class="table table-hover">
        <tbody id="list_post">
          <%= render  partial: '/shared/users/list_users', locals: {users:@users} %>
        </tbody>
      </table>
    </div>
    <div class="text-center">
      <%= paginate @users %>
    </div>
  </div>
</div>